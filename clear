[32m+[m[32m err.style.display = 'block'[m
[32m+[m[41m [m
[32m+[m
[32m+[m[41m [m
[32m+[m[32m}[m
[32m+[m[32mconst isValidEmail = email => {[m
[32m+[m[32m    const re = /^(([^<>()[\]\\.,;:\s@"]+(\.[^<>()[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;[m
[32m+[m[32m    return re.test(String(email).toLowerCase());[m
[32m+[m[32m}[m
[32m+[m[32mfunction success(element){[m
[32m+[m[32m const inputParent = element.parentElement[m
[32m+[m
[32m+[m
[32m+[m[32m const err = inputParent.querySelector('.error')[m
[32m+[m[32m inputParent.classList.add('success')[m
[32m+[m[32m inputParent.classList.remove('error')[m
[32m+[m
[32m+[m[32m if(err.style.display = 'block'){[m
[32m+[m[32m  err.style.display = 'none'[m
[32m+[m[32m }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mform.addEventListener('submit',e => {[m
[32m+[m[32m e.preventDefault()[m
[32m+[m[32m inputValidation()[m
[32m+[m[32m})[m
[32m+[m
[32m+[m
[32m+[m[32mfunction inputValidation(){[m
[32m+[m[32m const n = userName.value.trim()[m
[32m+[m[32m const e = email.value.trim()[m
[32m+[m[32m const m = month.value.trim()[m
[32m+[m[32m const d = day.value.trim()[m
[32m+[m[32m const y = year.value.trim()[m
[32m+[m[32m const h = hour.value.trim()[m
[32m+[m[32m const mi =minute.value.trim()[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[41m [m
[32m+[m[41m [m
[32m+[m
[32m+[m[32m if(n === ''){[m
[32m+[m[32m  error(userName)[m
[32m+[m[32m }else{[m
[32m+[m[32m  success(userName)[m
[32m+[m[32m }[m
[32m+[m
[32m+[m[32m if(e === ''){[m
[32m+[m[32m  error(email)[m
[32m+[m[32m }else if(!isValidEmail(e)){[m
[32m+[m[32m  error(email , 'Please Provide valid Email')[m
[32m+[m[32m }else{[m
[32m+[m[32m  success(email)[m
[32m+[m[32m }[m
[32m+[m[41m [m
[32m+[m[32m const checkingTime =moment(`${m}/${d}/${y}`,'MM/DD/YYYY',true).isValid()[m
[32m+[m
[32m+[m[32m if(m === ''|| y === '' || d === ''){[m
[32m+[m[32m    error(label,'This field is incomplete')[m
[32m+[m[32m }[m
[32m+[m[32m else if(!checkingTime){[m
[32m+[m[32m    error(label,'Please provide valid date')[m
[32m+[m[32m }[m
[32m+[m[32m else{[m
[32m+[m[32m    success(label)[m
[32m+[m[32m }[m
[32m+[m
[32m+[m[32m if(h === '' || mi === ''){[m
[32m+[m[32m    error(label2,'This field is incomplete')[m
[32m+[m[32m }[m
[32m+[m[32m else{[m
[32m+[m[32m    success(label2)[m
[32m+[m[32m }[m
[32m+[m[32m if(count === 0){[m
[32m+[m[32m    alert(`Please choose How Many People Are Going With You if It's only you then choose 1 people Thank you `)[m
[32m+[m[32m }[m
[32m+[m
[32m+[m
[32m+[m[32m}[m
